{
  "name": "adithwidhiantara/crud",
  "description": "Generate Simple CRUD",
  "type": "package",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Adithwidhiantara\\Crud\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Adithwidhiantara\\Crud\\CrudServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Adith Widhiantara"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    }
  },
  "require": {
    "php": "^8.2",
    "illuminate/database": "^11.0|^12.0",
    "illuminate/http": "^11.0|^12.0",
    "illuminate/pagination": "^11.0|^12.0",
    "illuminate/routing": "^11.0|^12.0",
    "illuminate/support": "^11.0|^12.0"
  },
  "require-dev": {
    "laravel/pint": "dev-main",
    "orchestra/testbench": "^9.0"
  }
}